<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="emmanuelrosales.packagesapp.InsertPackage"
    android:background="@android:color/background_dark"
    android:focusable="false" >


    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/inputPLenght"
        android:layout_marginLeft="55dp"
        android:layout_marginStart="55dp"
        android:textColor="@android:color/background_dark"
        android:width="400dp"
        android:height="80dp"
        android:textSize="50dp"
        android:background="#FFFFFF"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="96dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/inputPWidth"
        android:layout_marginLeft="182dp"
        android:layout_marginStart="182dp"
        android:textColor="@android:color/background_dark"
        android:width="400dp"
        android:height="80dp"
        android:textSize="50dp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/inputPLenght"
        android:layout_toRightOf="@+id/inputPLenght"
        android:layout_toEndOf="@+id/inputPLenght" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/inputPHeight"
        android:textColor="@android:color/background_dark"
        android:width="400dp"
        android:height="80dp"
        android:textSize="50dp"
        android:background="#FFFFFF"
        android:layout_below="@+id/inputPLenght"
        android:layout_alignLeft="@+id/inputPLenght"
        android:layout_alignStart="@+id/inputPLenght"
        android:layout_marginTop="86dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/inpuPname"
        android:textColor="@android:color/background_dark"
        android:width="400dp"
        android:height="80dp"
        android:textSize="50dp"
        android:background="#FFFFFF"
        android:layout_alignBottom="@+id/inputPHeight"
        android:layout_alignLeft="@+id/inputPWidth"
        android:layout_alignStart="@+id/inputPWidth" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/inputDiasEntrega"
        android:textColor="@android:color/background_dark"
        android:width="400dp"
        android:height="80dp"
        android:textSize="50dp"
        android:background="#FFFFFF"
        android:layout_below="@+id/inputPHeight"
        android:layout_alignLeft="@+id/inputPHeight"
        android:layout_alignStart="@+id/inputPHeight"
        android:layout_marginTop="86dp" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/spinner"
        android:layout_alignTop="@+id/inputDiasEntrega"
        android:layout_alignLeft="@+id/inpuPname"
        android:layout_alignStart="@+id/inpuPname"
        android:background="#FFFFFF"
        android:layout_alignRight="@+id/inpuPname"
        android:layout_alignEnd="@+id/inpuPname" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Home"
        android:id="@+id/btnHomeInserP"
        android:background="#ffff1100"
        android:textColor="#ffffffff"
        android:width="150dp"
        android:height="70dp"
        android:onClick="jumpHome"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/insert"
        android:id="@+id/btnInsertP"
        android:width="200dp"
        android:height="80dp"
        android:textSize="50dp"
        android:textColor="#ffffffff"
        android:background="#ba888888"
        android:onClick="getVal"
        android:layout_above="@+id/btnHomeInserP"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/packageLenght"
        android:id="@+id/txtLenght"
        android:width="400dp"
        android:height="80dp"
        android:textColor="#ffffffff"
        android:textSize="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/txtPHeight"
        android:layout_alignStart="@+id/txtPHeight" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/packageHeight"
        android:id="@+id/txtPHeight"
        android:width="400dp"
        android:height="60dp"
        android:textSize="50dp"
        android:textColor="#ffffffff"
        android:layout_above="@+id/inpuPname"
        android:layout_toLeftOf="@+id/btnInsertP"
        android:layout_toStartOf="@+id/btnInsertP" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/packageWidth"
        android:id="@+id/txtPWidth"
        android:width="400dp"
        android:height="80dp"
        android:textColor="#ffffffff"
        android:textSize="50dp"
        android:layout_above="@+id/inputPWidth"
        android:layout_alignLeft="@+id/inputPWidth"
        android:layout_alignStart="@+id/inputPWidth" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/packageDescription"
        android:id="@+id/textView"
        android:width="400dp"
        android:height="80dp"
        android:textColor="#ffffffff"
        android:textSize="50dp"
        android:layout_alignTop="@+id/txtPHeight"
        android:layout_alignLeft="@+id/inpuPname"
        android:layout_alignStart="@+id/inpuPname"
        android:layout_alignRight="@+id/btnHomeInserP"
        android:layout_alignEnd="@+id/btnHomeInserP" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Delivery Date"
        android:id="@+id/txtDeliveryDateP"
        android:width="400dp"
        android:height="60dp"
        android:textColor="#ffffffff"
        android:textSize="50dp"
        android:layout_above="@+id/spinner"
        android:layout_alignLeft="@+id/txtPHeight"
        android:layout_alignStart="@+id/txtPHeight" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Owners"
        android:id="@+id/txtPOwners"
        android:width="400dp"
        android:height="60dp"
        android:textColor="#ffffffff"
        android:textSize="50dp"
        android:layout_above="@+id/spinner"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView" />

</RelativeLayout>
